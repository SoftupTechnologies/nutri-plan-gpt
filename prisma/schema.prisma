datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider        = "prisma-client-js"
}

model OpenAIResponseAnalytics {
  id             Int      @id @default(autoincrement())
  answer         String
  timeToRespond  Int
}

model ReplicateIngredientsImageResponseAnalytics {
  id             Int      @id @default(autoincrement())
  answer         String
  timeToRespond  Int
}

model ReplicateMealImageResponseAnalytics {
  id             Int      @id @default(autoincrement())
  answer         String[]
  timeToRespond  Int
}
